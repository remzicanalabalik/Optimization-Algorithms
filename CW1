function poisson(x, λ, f=4) {
    let p = Math.exp(-λ)
    for (let k=1; k<=x; k++) p = p*λ/k
    return p.toFixed(f)
}
function doTable() {
    const L = [0.5, 1, 1.5, 2, 2.5, 3]
    let a = L.map((λ) => 'λ='+λ)
    let str = header(a)
    for (let x=0; x<=8; x++) { 
       let a = L.map((λ) => poisson(x, λ))
       str += oneLine(a, x)
    }
    return str
}
